rules:
  rousseau:
    ignoreTypes:
      - sentence:start
      - sentence:end
      - sentence:uppercase
    showLevels:
      - suggestion
      - warning
      - error
  alex:
    allow: []
  common-misspellings:
    ignore: []
  stop-words:
    defaultWords: true
    skip:
      - Blockquote
  en-capitalization: true,
  en-spell: true,
  write-good:
    passive: true
    illusion: true
    so: true
    thereIs: true
    weasel: true
    tooWordy: true
    cliches: true
